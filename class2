def count_odd_even(nums):
    odd_count = 0
    even_count = 0
    for num in nums:
        if num % 2 == 0:
            even_count += 1
        else:
            odd_count += 1
    return odd_count, even_count
nums = [ 2,3,4,55,56,78,75,69,66,101,100]
odd_count, even_count = count_odd_even(nums)
print("Odd count:", odd_count)
print("Even count:", even_count)
